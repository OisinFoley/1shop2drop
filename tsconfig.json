{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react",
    "module": "es6",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "outDir": "./dist/",
    "skipLibCheck": true,
    "sourceMap": true,
    "target": "es5"
  },
  "exclude": ["node_modules"]
}
